\name{constantVector}
\alias{constantVector}
\title{
	Create a constant vector object
}
\description{
	Create a constant vector object \code{\linkS4class{SimVector}}
}
\usage{
constantVector(constant, ni)
}
\arguments{
  \item{constant}{
	The number or character that is used to be the constant
}
  \item{ni}{
	The number of items
}
}
\value{
	The constant vector object
}
\author{
    Sunthud Pornprasertmanit (University of Kansas; \email{psunthud@ku.edu})
}
\examples{
# This function is not public.

# constantVector(0, 5)
}
